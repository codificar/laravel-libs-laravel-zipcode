      "Codificar\\Geolocation\\": "vendor/geolocation/src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "pestphp/pest": "^1.0",
    "pestphp/pest-plugin-laravel": "^1.0"
  },    
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
